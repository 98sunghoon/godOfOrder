{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source" : "/",
        "destination" : "/loginForm",
        "type" : 301
      }
    ],
    "rewrites": [
      {
        "source": "/**",
        "function": "api1"
      }
    ]
  }
}
